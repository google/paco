<manifest package='com.google.android.apps.paco' xmlns:android='http://schemas.android.com/apk/res/android' android:versionCode='73' android:versionName='2.0.73'>

    <application android:icon='@drawable/paco64' android:debuggable='true' android:label='@string/app_name' android:theme='@style/PacoTheme'>
        <activity android:name='com.google.android.apps.paco.ExperimentManagerActivity'>
            <intent-filter>
                <action android:name='android.intent.action.MAIN'/>

                <category android:name='android.intent.category.LAUNCHER'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.ExperimentList'>
            <intent-filter>
                <action android:name='android.intent.action.VIEW'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.dir/vnd.google.paco.experiment'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.FindExperimentsActivity'>
            <intent-filter>
                <action android:name='android.intent.action.VIEW'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.dir/vnd.google.paco.experiment'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.ExploreDataActivity' android:configChanges='orientation|keyboard|keyboardHidden'>
            <intent-filter>
                <action android:name='android.intent.action.VIEW'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.dir/vnd.google.paco.experiment'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.ExperimentDetailActivity' android:label='Join Experiment'>
            <intent-filter>
                <action android:name='android.intent.action.VIEW'/>
                <action android:name='android.intent.action.EDIT'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.item/vnd.google.paco.experiment'/>
            </intent-filter>
            <intent-filter>
                <action android:name='android.intent.action.INSERT'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.dir/vnd.google.paco.experiment'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.ExperimentScheduleActivity' android:screenOrientation='portrait' android:configChanges='orientation|keyboardHidden' android:windowSoftInputMode='stateHidden'/>
        <activity android:name='com.google.android.apps.paco.InformedConsentActivity' android:screenOrientation='portrait' android:configChanges='orientation|keyboardHidden'/>
        <activity android:name='com.google.android.apps.paco.ExperimentExecutor' android:configChanges='orientation|keyboardHidden' android:label='Record Data in Experiment' android:windowSoftInputMode='stateHidden'>
            <intent-filter>
                <category android:name='android.intent.category.LAUNCHER'/>
            </intent-filter>
            <intent-filter>
                <action android:name='android.intent.action.VIEW'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.item/vnd.google.paco.experiment'/>
            </intent-filter>
            <intent-filter>
                <action android:name='com.google.android.apps.paco.LINKED'/>

                <category android:name='android.intent.category.DEFAULT'/>
                <category android:name='android.intent.category.BROWSABLE'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.FeedbackActivity' android:label='Visualize Experiment Data' android:noHistory='true'>
            <intent-filter>
                <category android:name='android.intent.category.LAUNCHER'/>
            </intent-filter>
            <intent-filter>
                <action android:name='android.intent.action.VIEW'/>

                <category android:name='android.intent.category.DEFAULT'/>

                <data android:mimeType='vnd.android.cursor.item/vnd.google.paco.experiment'/>
            </intent-filter>
        </activity>
        <activity android:name='VersionUpdateActivity' android:label='Update Activity'>
            <intent-filter>
                <action android:name='com.google.android.apps.paco.UPDATE'/>

                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.WelcomeActivity'/>
        <activity android:name='com.google.android.apps.paco.AccountChooser'/>
        
        <activity android:name='.ESMSignalViewer' android:screenOrientation='portrait' android:label='ESM Signal Viewer'>
            <intent-filter>
                <action android:name='com.google.android.apps.paco.ESM_SIGNALS'/>

                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.ServerConfiguration' android:label='Paco Server Configuration'>
            <intent-filter>
                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.RawDataActivity' android:label='Paco Raw Data Viewer'>
            <intent-filter>
                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </activity>
        <activity android:name='com.google.android.apps.paco.ContactOptionsActivity'/>
        <activity android:name='com.google.android.apps.paco.HelpActivity'/>
        
        <!--  services -->
        <service android:name='com.google.android.apps.paco.NotificationCreatorService'>
            <intent-filter>
                <action android:name='com.google.android.apps.paco.NotificationCreatorService'/>

                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </service>
        <service android:name='com.google.android.apps.paco.BeeperService'>
            <intent-filter>
                <action android:name='com.google.android.apps.paco.BeeperService'/>

                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </service>
        <service android:name='.SyncService'/>
        <service android:name='.ServerCommunicationService'/>
        <service android:name='.VersionCheckerService'/>

        <receiver android:name='com.google.android.apps.paco.AlarmReceiver'>
            <intent-filter>
                <action android:name='com.google.android.apps.paco.AlarmReceiver'/>

                <category android:name='android.intent.category.DEFAULT'/>
            </intent-filter>
        </receiver>
        <receiver android:name='com.google.android.apps.paco.NetworkChangeReceiver' android:process=':remote'>
            <intent-filter>
                <action android:name='android.net.conn.CONNECTIVITY_CHANGE'/>
            </intent-filter>
        </receiver>
        <receiver android:name='com.google.android.apps.paco.SystemChangeEventReceiver'>
            <intent-filter>
                <action android:name='android.intent.action.BOOT_COMPLETED'/>
            </intent-filter>
            <intent-filter>
                <action android:name='android.intent.action.TIMEZONE_CHANGED'/>
            </intent-filter>
            <intent-filter>
                <action android:name='android.intent.action.PACKAGE_REPLACED'/>

                <data android:path='com.google.android.apps.paco' android:scheme='package'/>
            </intent-filter>
        </receiver>

        <provider android:name='SignalProvider' android:exported='false' android:authorities='com.google.android.apps.paco.signal'/>
        <provider android:name='ExperimentProvider' android:exported='false' android:authorities='com.google.android.apps.paco.ExperimentProvider'/>
    </application>

    <uses-permission android:name='android.permission.GET_ACCOUNTS'>
    </uses-permission>
    <uses-permission android:name='android.permission.MANAGE_ACCOUNTS'>
    </uses-permission>
    <uses-permission android:name='android.permission.USE_CREDENTIALS'>
    </uses-permission>
    <uses-permission android:name='android.permission.ACCESS_NETWORK_STATE'/>
    <uses-permission android:name='android.permission.INTERNET'>
    </uses-permission>
    <uses-permission android:name='android.permission.VIBRATE'>
    </uses-permission>

    <uses-sdk android:minSdkVersion='8'>
    </uses-sdk>

    <uses-permission android:name='android.permission.ACCESS_FINE_LOCATION'>
    </uses-permission>
    <uses-permission android:name='android.permission.ACCESS_COARSE_LOCATION'>
    </uses-permission>
    <uses-permission android:name='android.permission.CAMERA'/>

    <uses-feature android:name='android.hardware.camera'/>
    <uses-feature android:name='android.hardware.camera.autofocus'/>

    <uses-permission android:name='android.permission.WRITE_EXTERNAL_STORAGE'/>
    <uses-permission android:name='android.permission.WAKE_LOCK'/>
    <uses-permission android:name='android.permission.RECEIVE_BOOT_COMPLETED'/>
    <uses-permission android:name="android.permission.READ_LOGS" />

</manifest>
