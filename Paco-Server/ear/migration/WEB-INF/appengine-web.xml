<appengine-web-app xmlns='http://appengine.google.com/ns/1.0'>
  <application>quantifiedself-staging</application>
  <module>migration</module>
  <version>211</version>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property value='WEB-INF/logging.properties' name='java.util.logging.config.file'/> 


    <property name="ae-cloudsql.database-url" value="jdbc:google:mysql://XXX" />     
    <property name="ae-cloudsql.database-username" value="XXX"/>
    <property name="ae-cloudsql.database-password" value="XXX"/>

    <property name="ae-connection-pool.maxconn" value="100"/>
    
    <property name="ae-cloudsql.local-database-url" value="jdbc:mysql://localhost:3306/pacodb?verifyServerCertificate=false&amp;useSSL=false"/>
    <property name="ae-cloudsql.local-database-username" value="XXXX"/>
    <property name="ae-cloudsql.local-database-password" value="XXXX"/>
  </system-properties>
  
  <ssl-enabled>true</ssl-enabled>
  <sessions-enabled>true</sessions-enabled>
  <precompilation-enabled>true</precompilation-enabled>
  <threadsafe>true</threadsafe>
  <instance-class>B8</instance-class>
  <basic-scaling>
    <max-instances>10</max-instances>
  </basic-scaling>
</appengine-web-app>

