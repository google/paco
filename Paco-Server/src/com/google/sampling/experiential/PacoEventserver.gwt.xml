<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="pacoserver">
  <!-- set-configuration-property name="xsiframe.failIfScriptTag' value='False' -->
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
<inherits name="com.google.gwt.resources.Resources" />
<inherits name="com.google.gwt.xml.XML" />
<inherits name="com.google.gwt.widgetideas.WidgetIdeas" />
<inherits name="com.google.gwt.visualization.Visualization" />
<inherits name="com.google.gwt.gen2.picker.Picker" />                      
<inherits name="com.google.gwt.http.HTTP" />
<inherits name="com.google.gwt.json.JSON" />
<inherits name="com.google.gwt.i18n.I18N"/>
<inherits name="com.google.paco.shared.Shared" />
<inherits name='edu.ycp.cs.dh.acegwt.AceGWT'/>
<inherits name='com.google.gwt.maps.Maps' />
 
<extend-property name="locale" values="fi"/>
 <extend-property name="locale" values="ja"/>
  <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="pt"/>
<set-property-fallback name="locale" value="en"/>

<!-- <set-configuration-property name="locale.usemeta" value="N"/> -->
 <set-configuration-property name="locale.searchorder" value="queryparam,useragent,cookie"/>

<!-- <inherits name="com.google.gwt.maps.GoogleMaps" /> -->
<!-- <inherits name='com.google.gwt.maps.Apis_Google_Maps' /> -->

<inherits name="com.google.common.collect.Collect" />


 <!--
    If you want to deploy this application outside of localhost,
    you must obtain a Google Maps API key at:
    http://www.google.com/apis/maps/signup.html
    Replace the src attribute below with a URL that contains your key.
  -->
<!--
<script src="https://maps-api-ssl.google.com/maps/api/js?v=3&amp;sensor=false"></script>
--> 


  <!-- Specify the app entry point class. -->
  <entry-point class='com.google.sampling.experiential.client.DispatchEntryPoint'/>
  <source path='client'/>
  <source path='shared'/>
</module>
